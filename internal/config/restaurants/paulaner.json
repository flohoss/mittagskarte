      },
      {
        "day": { "fixed": "Woche" },
        "name": { "regex": "€.*\n{1,3}((?:.*\n){1,3})€" },
        "price": {
          "regex": "€.*\n{1,3}(?:.*\n){1,3}€\\s?(\\d{1,2}\\,\\d{1,2})"
        }
      },
      {
        "day": { "fixed": "Vegetarisch" },
        "name": { "regex": "Vegetarisch.*\n{1,2}((?:.*\n){1,3})€" },
        "price": {
          "regex": "Vegetarisch.*\n{1,2}(?:.*\n){1,3}€\\s?(\\d{1,2}\\,\\d{1,2})"
        }
      }
    ]
