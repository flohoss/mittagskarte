{
  "id": "meet-and-eat",
  "icon": "fa-solid fa-hotdog",
  "price": 1,
  "name": "Meet & Eat",
  "description": "Tankstelle",
  "page_url": "http://meetundeat.com/#Speisekarte",
  "address": "Schelmenwasenstraße 7, 70567 Stuttgart-Fasanenhof",
  "rest_days": ["Sunday", "Saturday"],
  "phone": "+49 157 30664255",
  "group": "Fasanenhof",
  "parse": {
    "http_version": "HTTP/1.1",
    "description": { "regex": "Speisekarte.*" },
    "navigate": [
      {
        "jquery": "img[sizes*='((min-width: 0px) and (max-width: 480px)) 480px, (min-width: 481px) 945px, 100vw']",
        "attribute": "src"
      }
    ],
    "is_file": true,
    "cropping": [
      { "crop": "100x10%+0+0" },
      { "crop": "50x60%+0+120" },
      { "gravity": "NorthEast", "crop": "50x60%+0+120" },
      { "gravity": "South", "crop": "50x30%+0+0" }
    ],
    "one_for_all": {
      "regex": "(.+?)€\\s?(\\d{1,2},\\d{2})((?:\\s?[a-z\\&].{4,}){0,3})",
      "pos_food": 1,
      "pos_price": 2,
      "pos_description": 3
    }
  }
}
